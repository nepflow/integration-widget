  "repository": {
    "type": "git",
    "url": "git+https://github.com/nepflow/integration-widget.git"
  },
  "scripts": {
    "dev": "http-server ./dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
